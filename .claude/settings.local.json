      "Bash(then)",
      "Bash(if grep -q \"from ''../../../lib/\" \"$dir[id].ts\")",
      "Bash(echo:*)",
      "Bash(fi)",
      "Bash(find:*)",
      "Bash(git add:*)",
      "Bash(npx wrangler deploy:*)",
      "Bash(git commit:*)"
    ]
  }
}
